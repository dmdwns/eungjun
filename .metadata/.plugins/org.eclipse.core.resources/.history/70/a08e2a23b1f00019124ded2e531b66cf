		ArrayList list1 = new ArrayList();
		list1.add(0,"홍길동" );
		list1.add(1, 100 );
		list1.add(2, 90 );
		list1.add(3, 81 );
		
		ArrayList list2 = new ArrayList();
		list2.add(0,"이순신");
		list2.add(1 , 95);
		list2.add(2, 88);
		list2.add(3, 92);
		
		ArrayList list3 = new ArrayList();
		list3.add(0 , "김유신 ");
		list3.add(1, 90);
		list3.add(2, 87);
		list3.add(3, 77);
//		김유신,  90,   87,   77
		
	
	    
	    String a1 =(String)list1.get(0); 
        int a2 = (int)list1.get(1);
        int a3 = (int)list1.get(2);
        int a4 = (int)list1.get(3);
        String b1 =(String)list1.get(0); 
        int b2 = (int)list1.get(1);
        int b3 = (int)list1.get(2);
        int b4 = (int)list1.get(3);
        String c1 =(String)list1.get(0); 
        int c2 = (int)list1.get(1);
        int c3 = (int)list1.get(2);
        int c4 = (int)list1.get(3);
	   
		StudentVO p0 = new StudentVO(list1.get(0),list1.get(1),list1.get(2),list1.get(3));
		StudentVO p1 = new StudentVO(list2.get(0),list2.get(1), list2.get(2), list2.get(3));
		StudnetVO p2 = new StudentVO(list3.get(0), list3.get(2), list3.get(3), list3.get(4));
		
		
		System.out.println();
		System.out.println(p0.getName()+"	"+p0.getKor()+"	"+p0.getEng()+"	"+p1.getMath()+
