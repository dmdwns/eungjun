package admin;

public class concertTotalVO {

	String concert_name;
	int concert_num;
	int total_cnt;
	int total_price;
	int seat_sum;
	int seat_per;
	
	
	
	public concertTotalVO(String concert_name, int concert_num, int total_cnt, int total_price, int seat_sum,
			int seat_per) {
		super();
		this.concert_name = concert_name;
		this.concert_num = concert_num;
		this.total_cnt = total_cnt;
		this.total_price = total_price;
		this.seat_sum = seat_sum;
		this.seat_per = seat_per;
	}
	public String getConcert_name() {
		return concert_name;
	}
	public int getConcert_num() {
		return concert_num;
	}
	public int getTotal_cnt() {
		return total_cnt;
	}
	public int getTotal_price() {
		return total_price;
	}
	public int getSeat_sum() {
		return seat_sum;
	}
	public int getSeat_per() {
		return seat_per;
	}
	@Override
	public String toString() {
		return   concert_num  + "\t" +concert_name+ "\t"
				+ total_cnt +"\t"+total_price +"\t" + seat_sum + "\t" +   seat_per  +  "\n"
				;
	}
	

	
	

	
}
