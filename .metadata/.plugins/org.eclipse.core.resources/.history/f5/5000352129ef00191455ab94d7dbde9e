package com.mystudy.ex03_abstract_class;

public class TypeTest {

	public static void main(String[] args) {
	//	Animal an = new Animal(); // 추상클래스는 객체생성 안됨 . 
    //  추상클래스는 공통적인것을 모아놓음 
	//  
		Cat cat = new Cat();
		Dog dog = new Dog();
		cat.sound();
		
		sound(cat);
	}
	
  static void sound(Animal animal ) {
	  animal.sound(); // 메소드 오버라이딩 기능 사용 
  
  }
}
