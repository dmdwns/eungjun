package com.mystudy.ex01_bean;

public class StudentMain {

	public static void main(String[] args) {
		Student stu = new Student();
//		stu.name = "홍길동 ";
//		System.out.println(stu.name);
		stu.setName("홍길동2");  //값을 읽고
		System.out.println(stu.getName());  // 값을 설정 
		
		stu.setKor(100);
		System.out.println("국어점수" +stu.getKor());

		stu.setMath(90);
		System.out.println("수학점수" +stu.getMath());

		stu.setEng(85);
		System.out.println("영어점수" +stu.getEng());

			
		// 3개의 매개변수에 단순숫자말고  다른저장값으로 가능?...
		stu.computeAvg(); //여기서 계산함 
 		System.out.println("총점 : " +stu.getTot());
        // 여기까지 다됨 ..		
		System.out.println("평균 : " +stu.getAvg());
			
		
		Student stu2 = new Student("김유신 " , 100 , 90 ,80);
		
		
		
	}

	
	
}
