//  퍼블릭 클래스는 하나만 
public class InstanceClassTest {

	int a = 100;
	private int b = 10;
	static int c = 200;
	int sum = 0;
	
	void sum() {
		sum = a + b;
	}
	
	// 내부클래스 : 인스턴스 내부클래스
	class Inner {
		
		
	}
	
	
	
	
	public static void main(String[] args) {
		
		

	}

}
