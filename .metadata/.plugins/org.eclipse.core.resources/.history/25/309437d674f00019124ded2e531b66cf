package com.mystudy.ex04_interface;

public abstract class Animal implements I_Animal{
   // I_animal타입에 animal도 포함 으로본다 
	
	@Override
	public void eat() {
		System.out.println(">> 먹는다");
		
	}
	@Override
	public void sleep() {
		System.out.println(">> 잠을잔다");
		
	}
	@Override
	public abstract void sound(); //선언만 하고 구현은 하지 않음 
     // 원래 클래스에서 그렇게 쓰였음

	
}


